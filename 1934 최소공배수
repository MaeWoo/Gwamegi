#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int t, a, b, cnt = 2, lmulti=1;
	scanf("%d", &t);
	for (int i = 0; i < t; i++) {
		scanf("%d %d", &a, &b);
		lmulti = 1;
		cnt = 2;
		while (1) {
			if (a % cnt == 0 && b % cnt == 0) {
				lmulti *= cnt;
				a /= cnt; b /= cnt;
			}
			else if (a % cnt == 0 && b % cnt != 0) {
				lmulti *= cnt;
				a /= cnt;
			}
			else if (a % cnt != 0 && b % cnt == 0) {
				lmulti *= cnt;
				b /= cnt;
			}
			else {
				cnt++;
			}
			if (cnt > a && cnt > b) {
				printf("%d\n", lmulti);
				break;
			}
		}
	}
}
