#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>	

int main() {
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n*2-1; i++) {
		if (i < n) {
			for (int j = 0; j < n - (n - i) + 1; j++) {
				printf("*");
			}
			printf("\n");
		}
		else {
			for (int j = n-(i-n)-1; j > 0; j--) {
				printf("*");
			}
			printf("\n");
		}
	}
}
