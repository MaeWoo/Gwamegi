#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>

int main() {
	int n, temp, price, top = -1;
	int stack[1000000] = { 0 };
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		scanf("%d", &temp);
		if (temp == 1) {
			scanf("%d", &price);
			stack[++top] = price;
		}
		if (temp == 2) {
			if (top == -1) {
				printf("-1\n");
			}
			else {
				printf("%d\n", stack[top--]);
			}
		}
		if (temp == 3) {
			printf("%d\n", top + 1);
		}
		if (temp == 4) {
			if (top == -1)
				printf("1\n");
			else
				printf("0\n");
		}
		if (temp == 5) {
			if (top == -1) {
				printf("-1\n");
			}
			else {
				printf("%d\n", stack[top]);
			}
		}
	}
}
