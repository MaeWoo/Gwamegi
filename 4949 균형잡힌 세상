#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int top = -1, check = 0;
    char stack[101] = { 0 };
    char arr[101] = { 0 };

    while (1) {
        gets(arr);
        if (arr[0] == '.') {
            break;
        }
        for (int i = 0; i < strlen(arr); i++) {

            if (arr[i] == '(') {
                stack[++top] = '(';
            }
            else if (arr[i] == '[') {
                stack[++top] = '[';
            }
            if (arr[i] == ')') {
                if (stack[top] != '(') {
                    printf("no\n");
                    check++;
                    break;
                }
                else {
                    top--;
                }
            }
            else if (arr[i] == ']') {
                if (stack[top] != '[') {
                    printf("no\n");
                    check++;
                    break;
                }
                else {
                    top--;
                }
            }
        }
        if (top == -1 && check == 0) {
            printf("yes\n");
        }
        else if (top != -1 && check == 0) {
            printf("no\n");
        }
        top = -1;
        check = 0;
    }
}
