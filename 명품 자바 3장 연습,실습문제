package practice;

import java.util.Scanner;
public class practice3 {

}

class first{
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.print("알파벳 한 문자를 입력하세요>>");
		String a = scan.next();
		char b = a.charAt(0);
		
		for(int i=97;i<=b;i++) {
			for(int j=i;j<=b;j++) {
				System.out.print((char)j);
			}
			System.out.println();
		}
	}
}
